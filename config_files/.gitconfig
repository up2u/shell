[user]
	name = name
	email = name@mail.com
[color]
	status = auto
	diff = auto
	branch = auto
	ui = auto
	interactive = auto
[alias]
	a   = add
	au  = add -u
	d   = diff
	dw  = diff -w
	dc  = diff --cached
	dcw = diff --cached -w
	co  = checkout
	st  = status
	br  = branch
	cmt = commit
	rmt = remote
[core]
	excludesfile = ~/.gitignore
	whitespace = cr-at-eol
#   editor = emacs
[commit]
#   template = ~/.gitcommitmsg.txt
	pager = less
